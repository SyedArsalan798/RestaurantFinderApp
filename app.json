{
  "expo": {
    "name": "FoodieApp",
    "slug": "foodieapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/splash2.png",
        "resizeMode": "cover",
        "backgroundColor": "#000"
      },
      "icon": "./assets/Icon.png",
      "package": "com.arsalanmunir.FoodieApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "597743ee-bcb0-4072-b490-c97ec403199f"
      }
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "MAPBOX_PRIVATE_KEY"
        }
      ]
    ],
    "owner": "arsalanmunir"
  }
}
